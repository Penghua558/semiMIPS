    %vpi_call 2 30 "$monitor", "%g\011 %b\011 %b\011 %b\011 %d\011 %d\011\012", $time, v0x555f3b434f10_0, v0x555f3b434c70_0, v0x555f3b435000_0, v0x555f3b434d40_0, v0x555f3b434e40_0 {0 0 0};
    %delay 7, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x555f3b434c70_0, 0, 1;
    %delay 68, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555f3b434c70_0, 0, 1;
