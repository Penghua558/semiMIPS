    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x555f3b434f10_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 8, 0, 5;
    %store/vec4 v0x555f3b434d40_0, 0, 5;
