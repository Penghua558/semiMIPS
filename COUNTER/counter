v0x555f3b434a30_0 .net "load", 0 0, v0x555f3b435000_0;  1 drivers
E_0x555f3b3e0db0/0 .event negedge, v0x555f3b434690_0;
E_0x555f3b3e0db0/1 .event posedge, v0x555f3b41c540_0;
E_0x555f3b3e0db0 .event/or E_0x555f3b3e0db0/0, E_0x555f3b3e0db0/1;
    .scope S_0x555f3b41c1d0;
T_0 ;
    %wait E_0x555f3b3e0db0;
    %load/vec4 v0x555f3b434690_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0x555f3b434840_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x555f3b434920_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x555f3b434a30_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x555f3b434750_0;
    %assign/vec4 v0x555f3b434840_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x555f3b434840_0;
    %pad/u 32;
    %cmpi/u 28, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.6, 5;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0x555f3b434840_0, 0;
    %jmp T_0.7;
T_0.6 ;
    %load/vec4 v0x555f3b434840_0;
    %addi 4, 0, 5;
    %assign/vec4 v0x555f3b434840_0, 0;
T_0.7 ;
T_0.5 ;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x555f3b409270;
T_1 ;
    %delay 4, 0;
    %load/vec4 v0x555f3b434bb0_0;
    %inv;
    %store/vec4 v0x555f3b434bb0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x555f3b409270;
T_2 ;
